alias la="ls -la --size"
alias jhreplace="jhbuild run ./home/magpie/gnome/gnome-shell/src/gnome-shell --replace"
alias gsreplace="./usr/bin/gnome-shell --replace"
alias pk="pkcon search name"
alias avail="grep Available"
alias in="grep Installed"					
alias takefile="sudo chown magpie:magpie"
alias userruns="ps -fe | grep "
alias eget="sudo easy_install"
alias get="sudo yum install -y"
alias acf_gs="jhbuild buildone -afc gnome-shell"
alias g_s="jhbuild build gnome-shell"
alias cln="git clean -X -f -d"
alias distcln="make distclean"
alias shelldir="cd /home/magpie/gnome/gnome/shell"

alias track="gjs /home/magpie/gnome/gnome-shell/js/ui/focusCaretTracker.js"
alias mag="gjs /home/magpie/gnome/gnome-shell/js/ui/magnifier.js"
alias track-lint="gjslint /home/magpie/gnome/gnome-shell/js/ui/focusCaretTracker.js"
alias mag-lint="gjslint /home/magpie/gnome/gnome-shell/js/ui/magnifier.js"
alias jh="./configure --prefix=/home/magpie/gnome/install --libdir /home/magpie/gnome/install/lib64 --disable-static"
